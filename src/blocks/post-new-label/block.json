{
	"apiVersion": 2,
	"name": "fse-blocks/post-new-label",
	"title": "New Post Label",
	"category": "theme",
	"keywords": [ "new", "post"],
	"description": "Display a label for new posts.",
	"textdomain": "fse-blocks",
	"attributes": {
		"contentJustification": {
			"type": "string"
		},
		"day": {
			"type":"number"
		},
		"label": {
			"type": "string",
			"default": "New"
		},
		"textAlign": {
			"type": "string",
			"default": "center"
		},
		"width": {
			"type": "number"
		},
		"widthUnit": {
			"type": "string",
			"default": "px"
		}
	},
	"usesContext": [ "postId", "postType", "queryId" ],
	"supports": {
		"html": false,
		"color": {
			"gradients": true,
			"__experimentalSkipSerialization": true
		},
		"spacing": {
			"padding": [ "horizontal", "vertical" ],
			"margin": true,
			"__experimentalSkipSerialization": true
		},
		"typography": {
			"lineHeight": true,
			"fontSize": true,
			"__experimentalFontStyle": true,
			"__experimentalFontWeight": true,
			"__experimentalLetterSpacing": true,
			"__experimentalTextTransform": true
		},
		"__experimentalBorder": {
			"color": true,
			"radius": true,
			"style": true,
			"width": true,
			"__experimentalSkipSerialization": true
		},
		"__experimentalSelector": ".wp-block-fse-blocks-post-new-label__inner"
	},
	"editorScript": "file:../../../build/post-new-label.js",
	"editorStyle": "file:../../../build/post-new-label.css",
	"style": "file:../../../build/style-post-new-label.css"
}
